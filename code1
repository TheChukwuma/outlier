function outlier(arg){
    var even = [];
    var odd = []
    for (let i=0; i < arg.length; i++){
        if (arg[i]%2==0){
            even.push(arg[i])}
        else {
            odd.push(arg[i]);}
    }
    if (even.length > odd.length) {
        return `${odd[0]} (the only odd number)`;}
    else{
        return `${even[0]} (the only even number)`;}
}
